@model List<AllSubCategoriesAndCategoriesForm>

@{
    ViewData["Title"] = "SubCategories";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<h1>
    SubCategories
</h1>
<a asp-action="Create">Create New</a>

<table class="table table-stripped">
    <thead>
        <tr>
            <th>id</th>
            <th>Name</th>
            <th>Category</th>
            <th>Details</th>
            <th>Edit</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var SubCat in Model)
        {
            <tr>
                <td>@SubCat.id</td>
                <td>@SubCat.name</td>
                <td>@SubCat.CatName</td>
                <td>
                    <a class="btn btn-primary" asp-action="Details" asp-route-id="@SubCat.id">Details</a>
                </td>
                <td>
                    <a class="btn btn-primary" asp-action="Edit" asp-route-id="@SubCat.id">Edit</a>
                </td>
                <td>
                    <a class="btn btn-primary" asp-action="Delete" asp-route-id="@SubCat.id">Delete</a>
                </td>

            </tr>
        }
    </tbody>

</table>